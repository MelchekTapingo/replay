.customGglBtn
  display: inline-block;
  background: white;
  color: #444;
  border: thin solid #888;
  box-shadow: 1px 1px 1px grey;
  white-space: nowrap;
.customGglBtn:hover
  cursor: pointer;
span.label
  font-family: serif;
  font-weight: normal;
span.icon
  background: url('./img/g-normal.png') transparent 5px 50% no-repeat;
  display: inline-block;
  vertical-align: middle;
  width: 30px;
  height: 25px;
span.buttonText
  display: inline-block;
  vertical-align: middle;
  padding-left: 5px;
  padding-right: 20px;
  font-size: 14px;
  font-weight: bold;
  /* Use the Roboto font that is loaded in the <head> */
  font-family: 'Roboto', sans-serif;
